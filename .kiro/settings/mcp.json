{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=your_project_ref_here"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "your_supabase_access_token_here"
      },
      "disabled": false,
      "autoApprove": ["search_docs"]
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "disabled": false,
      "autoApprove": []
    },
    "stripe": {
      "command": "npx",
      "args": ["-y", "@stripe/mcp", "--tools=all"],
      "env": {
        "STRIPE_SECRET_KEY": "your_stripe_test_key_here"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
